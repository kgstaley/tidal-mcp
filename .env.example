# TIDAL MCP Configuration
# Copy this file to .env and fill in your values.

# Port for the Flask backend (default: 5050)
TIDAL_MCP_PORT=5100

# Custom OAuth credentials (optional).
# If not set, tidalapi's built-in defaults are used.
# Set these to avoid breakage when tidalapi's hardcoded credentials get revoked.
TIDAL_CLIENT_ID=
TIDAL_CLIENT_SECRET=
